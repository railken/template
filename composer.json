{
    "name": "railken/template",
    "keywords": ["template"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "railken/bag": "^2.0",
        "rcrowe/twigbridge": "^0.14.2",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "spatie/pdf-to-text": "^1.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Template\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Railken\\Template\\Tests\\": "tests/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Template\\TemplateServiceProvider"
            ]
        }
    }
}
